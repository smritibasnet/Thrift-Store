{
  "dependencies": {
    "axios": "^0.26.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "knex": "^1.0.3",
    "mysql2": "^2.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.5",
    "@babel/node": "^7.16.8",
    "@babel/preset-env": "^7.16.11",
    "concurrently": "^7.0.0",
    "nodemon": "^2.0.15",
    "rimraf": "^3.0.2",
    "sequelize-cli": "^6.4.1"
  },
  "name": "thriftstore",
  "version": "1.0.0",
  "description": "Final Year Project on thrift shopping cart app",
  "main": "server.js",
  "repository": "https://github.com/smritibasnet/Thrift-Store",
  "author": "Smriti Basnet",
  "license": "MIT",
  "scripts": {
    "knex": "babel-node node_modules/.bin/knex",
    "start:dev": "nodemon --watch .env --watch src --exec babel-node src",
    "make:seeder": "yarn knex seed:make --knexfile src/knexfile.js",
    "make:migration": "yarn knex migrate:make --knexfile src/knexfile.js",
    "migrate": "yarn knex migrate:latest --knexfile src/knexfile.js",
    "rollback": "yarn knex migrate:rollback --knexfile src/knexfile.js"
  }
}
